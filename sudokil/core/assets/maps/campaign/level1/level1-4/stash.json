{
	deviceName : stash,
	name : stash,
	children: 
	[	
		{
			class: ScriptCLI,
			name: lift.sh,
			usage: "lift.sh \n\t description: lifts the object directly in front of the robot",
			event:
			{
				class: LiftEvent
			}
		},
		{
			class: ScriptCLI,
			name: lower.sh,
			usage: "lower.sh \n\t description: lowers the object directly in front of the robot",
			event:
			{
				class: LowerEvent
			}
		},
	]
}